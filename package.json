  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "^5.34.0",
    "esbuild": "^0.15.5",
    "esbuild-plugin-ifdef": "^1.0.1",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4",
    "yargs": "^17.5.1"
