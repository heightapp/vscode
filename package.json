  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/cjs/index.js",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "NODE_ENV=production ./scripts/build.js --cjs",
